{
  "name": "railway-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Railway code-server environment access",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "prepare": "npm run build",
    "enhanced": "node dist/index-enhanced.js",
    "csv": "node dist/index-csv-enhanced.js",
    "csv:dev": "tsx src/index-csv-enhanced.ts",
    "agent:interactive": "ts-node src/ai-agent-orchestrator.ts interactive",
    "agent:automated": "ts-node src/ai-agent-orchestrator.ts automated",
    "agent:dev": "tsx src/ai-agent-orchestrator.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "express": "^4.18.2",
    "multer": "^1.4.4",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.10.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
